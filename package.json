{
  "name": "simple-side-menu",
  "version": "1.0.12",
  "description": "Simple side menu",
  "private": false,
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "audit:fix": "npm audit fix --force",
    "prettify": "npx prettier '**/*.{js,ts,tsx,mjs,cjs,json}' --write",
    "clean:all": "rm -rf node_modules package-lock.json dist && npm cache clean -f",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:style": "sass src/styles/main.scss:src/index.css --no-source-map --style compressed",
    "build": "npm i && mkdir -p dist && npm run build:style && cp src/index.css dist/index.css && tsc && rm -rf node_modules",
    "test": "jest",
    "test:coverage": "npm run clean && npm run build && npm run test -- --coverage",
    "prepare": "husky install",
    "example": "cd example && npm start"
  },
  "author": {
    "name": "Hamza Mhadbi",
    "email": "hamza.mhadbi17@gmail.com"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HamzaMhadbi/simple-side-menu"
  },
  "keywords": [
    "react",
    "sidebar",
    "sidemenu",
    "sidenav",
    "navigation",
    "navigation-drawer",
    "react-router"
  ]
}
